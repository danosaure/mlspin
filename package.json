{
  "name": "@danosaure/mlspin",
  "version": "1.0.0",
  "description": "mlspin db",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "check": "run-s eslint prettier",
    "clean:build": "rimraf build",
    "clean:docs": "rimraf docs",
    "clean": "run-s clean:build clean:docs",
    "coverage": "nyc npm test",
    "eslint": "eslint --fix --ext=.js,.ts,.tsx src",
    "prettier": "prettier 'src/**/*.ts' --write",
    "sass": "sass src/components/style.scss docs/style.css",
    "test": "mocha",
    "pretsc": "run-s clean:build",
    "tsc": "tsc",
    "webpack:analyse": "webpack --json > reports/webpack.analyse.json",
    "webpack": "webpack",
    "postwebpack": "rimraf docs/style.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danosaure/mlspin.git"
  },
  "keywords": [
    "mlspin",
    "db"
  ],
  "author": {
    "name": "Danosaure",
    "email": "1444063+danosaure@users.noreply.github.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danosaure/mlspin/issues"
  },
  "homepage": "https://github.com/danosaure/mlspin#readme",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@mui/x-data-grid": "^6.19.1",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-direct-import": "^1.0.0",
    "babel-plugin-import": "^1.13.8",
    "chai": "^4.4.1",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "html-webpack-plugin": "^5.6.0",
    "install": "^0.13.0",
    "mini-css-extract-plugin": "^2.7.7",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "max-len": "off",
      "no-console": "warn",
      "no-async-promise-executor": "warn",
      "prettier/prettier": "error",
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@mui/*/*/*"
          ]
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.test.ts"
        ],
        "env": {
          "mocha": true
        },
        "globals": {
          "expect": "readonly"
        }
      }
    ]
  },
  "nyc": {
    "all": true,
    "temp-dir": "./reports/nyc_output",
    "report-dir": "./reports/coverage",
    "check-coverage": false,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.*.test.ts",
      "src/**/*.*.test.tsx"
    ],
    "reporter": [
      "cobertura",
      "lcov",
      "html"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 132
  }
}
